  // ============================================================================
  // ICON COLOR MAPPINGS
  // ============================================================================
  "iconColors": {
    "semantic": {
      "success": "#28a745",   // Green - bi-check-circle, bi-play-fill
      "info": "#17a2b8",      // Cyan - bi-info-circle, bi-save
      "warning": "#ffc107",   // Yellow - bi-star, bi-exclamation-triangle
      "danger": "#dc3545",    // Red - bi-x-circle, bi-trash, bi-diagram-3 (Schema Generator)
      "primary": "#007bff",   // Blue - bi-folder2-open, bi-file-earmark-code
      "secondary": "#6c757d", // Gray - bi-arrow-clockwise, bi-clock-history
      "purple": "#6f42c1",    // Purple - bi-search, bi-braces
      "teal": "#20c997",      // Teal - bi-code-square
      "orange": "#fd7e14"     // Orange - bi-arrow-repeat (XSLT)
    },
    "tabSpecific": {
      "xmlEditor": null,              // Default icon color
      "xsdEditor": "dodgerblue",      // Blue for XSD
      "schemaGenerator": "#dc3545",   // Red for Schema Generator
      "xslt": "#e27429",              // Orange
      "schematron": "#ff6b6b",        // Light red
      "fop": "INDIANRED",             // PDF
      "signature": "#578013",         // Green
      "help": "#5151e8"               // Violet/blue
    }
  },

  // ============================================================================
  // COMPONENTS
  // ============================================================================
  "components": {
    // Button Variants
    "buttons": {
      "default": {
        "background": "linear-gradient(from 0% 0% to 0% 100%, #ffffff, #f0f2f5)",
        "textColor": "#24292f",
        "borderColor": "#d0d7de",
        "borderRadius": "4px",
        "padding": "5px 15px",
        "fontWeight": "bold"
      },
      "primary": {
        "background": "linear-gradient(from 0% 0% to 0% 100%, #2b8aff, #0969da)",
        "textColor": "white",
        "borderColor": "#0550ae"
      },
      "success": {
        "background": "linear-gradient(to bottom, #28a745 0%, #1e7e34 100%)",
        "textColor": "white"
      },
      "warning": {
        "background": "linear-gradient(to bottom, #ffc107 0%, #e0a800 100%)",
        "textColor": "#212529"
      },
      "info": {
        "background": "linear-gradient(to bottom, #17a2b8 0%, #117a8b 100%)",
        "textColor": "white"
      },
      "outline": {
        "background": "transparent",
        "textColor": "#4a90e2",
        "borderColor": "#4a90e2"
      },
      "menu": {
        "height": "50px",
        "width": "200px",
        "alignment": "baseline-left",
        "background": "transparent",
        "activeBackground": "#d0e7ff",
        "activeBorder": "0 0 0 3px #0969da"
      },
      "toolbar": {
        "background": "transparent",
        "borderRadius": "6px",
        "padding": "8px 10px",
        "fontSize": "9px"
      }
    },

    // Card Component
    "card": {
      "background": "#ffffff",
      "borderColor": "#e1e4e8",
      "borderRadius": "6px",
      "padding": "20px",
      "shadow": "dropshadow(gaussian, rgba(0, 0, 0, 0.05), 10, 0, 0, 2)"
    },

    // Action Card (Dashboard)
    "actionCard": {
      "background": "white",
      "borderColor": "#e5e7eb",
      "borderRadius": "8px",
      "padding": "16px",
      "minHeight": "160px",
      "iconWrapperSize": "48px",
      "hover": {
        "background": "#f8fafc",
        "borderColor": "#cbd5e1"
      }
    },

    // Tab Pane
    "tabPane": {
      "headerBackground": "#f6f8fa",
      "tabPadding": "8px 12px",
      "selectedBorder": "2px solid #0969da",
      "tabGroups": {
        "primary": "linear-gradient(to bottom, #f3e5f5, #e1bee7)",
        "output": "linear-gradient(to bottom, #e8f5e9, #c8e6c9)",
        "utility": "linear-gradient(to bottom, #e3f2fd, #bbdefb)"
      }
    },

    // Dialog
    "dialog": {
      "sizes": {
        "small": { "width": "400px", "height": "300px" },
        "medium": { "width": "600px", "height": "450px" },
        "large": { "width": "850px", "height": "650px" },
        "xlarge": { "width": "1000px", "height": "750px" }
      },
      "header": {
        "variants": {
          "primary": "linear-gradient(to bottom, var(--color-primary), derive(var(--color-primary), -20%))",
          "success": "linear-gradient(to bottom, var(--color-success), derive(var(--color-success), -20%))",
          "info": "linear-gradient(to bottom, var(--color-info), derive(var(--color-info), -20%))",
          "warning": "linear-gradient(to bottom, var(--color-warning), derive(var(--color-warning), -20%))",
          "danger": "linear-gradient(to bottom, var(--color-danger), derive(var(--color-danger), -20%))"
        }
      },
      "infoBox": {
        "info": { "background": "#e3f2fd", "border": "#90caf9", "text": "#1976d2" },
        "success": { "background": "#e8f5e9", "border": "#a5d6a7", "text": "#2e7d32" },
        "warning": { "background": "#fff3e0", "border": "#ffcc80", "text": "#e65100" },
        "danger": { "background": "#ffebee", "border": "#ef9a9a", "text": "#c62828" }
      }
    },

    // Context Menu
    "contextMenu": {
      "background": "#f5f5f5",
      "borderRadius": "6px",
      "borderColor": "#d0d7de",
      "shadow": "dropshadow(gaussian, rgba(0, 0, 0, 0.1), 10, 0, 0, 2)",
      "menuItem": {
        "padding": "8px 12px",
        "hoverBackground": "rgba(74, 144, 226, 0.1)"
      },
      "iconClasses": {
        "success": ".menu-icon-success",
        "info": ".menu-icon-info",
        "warning": ".menu-icon-warning",
        "danger": ".menu-icon-danger",
        "primary": ".menu-icon-primary",
        "purple": ".menu-icon-purple",
        "orange": ".menu-icon-orange",
        "teal": ".menu-icon-teal"
      }
    },

    // Toolbar
    "toolbar": {
      "background": "linear-gradient(to bottom, #f8f9fa, #e9ecef)",
      "borderBottom": "1px solid #dee2e6",
      "padding": "6px 10px",
      "buttonSpacing": "4px",
      "iconStyles": {
        "success": "#28a745",
        "primary": "#007bff",
        "info": "#17a2b8",
        "warning": "#ffc107",
        "danger": "#dc3545",
        "teal": "#20c997",
        "purple": "#6f42c1",
        "secondary": "#6c757d"
      }
    },

    // Statistics Card (Dashboard)
    "statisticsCard": {
      "background": "#ffffff",
      "borderRadius": "8px",
      "padding": "16px",
      "valueSize": "28px",
      "labelSize": "12px",
      "trendColors": {
        "up": "#28a745",
        "down": "#dc3545",
        "neutral": "#6c757d"
      }
    },

    // Feature Progress Grid
    "featureProgressGrid": {
      "chipDiscovered": {
        "background": "#e7f3ff",
