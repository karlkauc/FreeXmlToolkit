  // ============================================================================
  // USAGE GUIDELINES
  // ============================================================================
  "guidelines": {
    "icons": [
      "Always use icons from Ikonli Bootstrap Icons (bi-*)",
      "Verify icon existence at https://kordamp.org/ikonli/cheat-sheet-bootstrapicons.html before use",
      "NEVER use bi-database - it does not exist!",
      "Standard icon size in FXML: iconSize='16'",
      "Toolbar icon size: iconSize='20'",
      "Always provide iconColor for semantic meaning",
      "Use styleClass for themed icon colors (e.g., toolbar-icon-success)"
    ],
    "colors": [
      "Use semantic colors for actions (success=green, danger=red, etc.)",
      "Use tab category colors for grouping related functionality",
      "Maintain consistency between light and dark themes",
      "Use CSS variables when possible for theme support"
    ],
    "components": [
      "Use standard button variants for consistent styling",
      "Action cards should have fixed height (160px) for dashboard layout",
      "Dialog headers should use gradient variants for visual hierarchy",
      "Toolbar icons should have colored styleClasses for semantic meaning"
    ],
    "bestPractices": [
      "All user-facing text must be in English",
      "Menus and context menus should always have icons and text",
      "Use dropshadow effects sparingly for depth",
      "Maintain 8px grid alignment for spacing",
      "Use linear gradients for button backgrounds"
    ]
  },

  // ============================================================================
  // COMMON ICON USAGE PATTERNS
  // ============================================================================
  "patterns": {
    "toolbarButton": {
      "example": "<FontIcon iconLiteral=\"bi-play-fill\" iconSize=\"20\" iconColor=\"#28a745\" styleClass=\"toolbar-icon-success\"/>",
      "notes": "Use styleClass for hover effects"
    },
    "menuItem": {
      "example": "<MenuItem text=\"Save\"><graphic><FontIcon iconLiteral=\"bi-save\" iconSize=\"16\"/></graphic></MenuItem>",
      "notes": "Always include both text and icon"
    },
    "actionCard": {
      "example": "<FontIcon iconLiteral=\"bi-plus\" iconSize=\"24\" iconColor=\"#2563eb\"/>",
      "notes": "Wrap in StackPane with colored background"
    },
    "statusIndicator": {
      "example": "<FontIcon iconLiteral=\"bi-check-circle-fill\" iconColor=\"#28a745\"/>",
      "notes": "Use fill variants for status indicators"
    }
  }
