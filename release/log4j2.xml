<Configuration status="ERROR">
    <Appenders>
        <File name="MyFile" fileName="logs//${date:yyyy-MM-dd-HH-mm}-fxt.log" immediateFlush="true">
            <PatternLayout pattern="%d{yyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </File>
    </Appenders>
    <Loggers>
        <Root level="ERROR">
        </Root>
    </Loggers>
</Configuration>